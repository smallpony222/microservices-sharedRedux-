{
  "homepage": "https://amitpatil321.github.io/shared-redux-state-into-micro-frontends",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:host": "cd host && npm install",
    "install:store": "cd store && npm install",
    "install:nav": "cd nav && npm install",
    "start": "npm-run-all --parallel start:*",
    "start:host": "cd host && npm start",
    "start:store": "cd store && npm start",
    "start:nav": "cd nav && npm start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "react-redux": "^9.1.2"
  }
}
